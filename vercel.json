{
  "builds": [
      {
          "src": "customer_service/wsgi.py",
          "use": "@vercel/python",
          "config": { "maxlambdaSize": "15mb", "runtime": "python3.9" }
      }
  ],
  "routes": [
      {
          "src": "/mtn_bot",
          "dest": "/chat.html"
      },
      {
          "src": "/admin",
          "dest": "/admin"
      },
      {
          "src": "/(.*)",
          "dest": "customer_service/wsgi.py"
      }
  ]
}

  